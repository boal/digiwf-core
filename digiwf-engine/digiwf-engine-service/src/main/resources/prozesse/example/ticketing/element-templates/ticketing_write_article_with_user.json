{
  "name": "Ticketing: Artikel mit Benutzer schreiben",
  "id": "de.muenchen.digitalwf.templates.ticketing.write-article-with-user",
  "appliesTo": [
    "bpmn:CallActivity"
  ],
  "properties": [
    {
      "label": "Template",
      "type": "String",
      "editable": false,
      "value": "StreamingTemplateV02",
      "binding": {
        "type": "property",
        "name": "calledElement"
      }
    },
    {
      "label": "Integration",
      "type": "String",
      "value": "ticketIntegration",
      "editable": false,
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "app_integration_name",
        "target": "app_integration_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Type Header",
      "type": "String",
      "editable": false,
      "value": "writeArticle",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "app_type_name",
        "target": "app_type_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Ticket ID",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "ticketId",
        "target": "ticketId"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Article",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "article",
        "target": "article"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Status",
      "type": "Dropdown",
      "value": "-",
      "choices": [
        {
          "name": "Offen",
          "value": "OPEN"
        },
        {
          "name": "Ausstehende Erinnerung",
          "value": "PENDING_REMINDER"
        },
        {
          "name": "Bis zum Abschluss",
          "value": "PENDING_CLOSE"
        },
        {
          "name": "Geschlossen",
          "value": "CLOSED"
        }
      ],
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "status",
        "target": "status"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "User",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "userId",
        "target": "userId"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Pfad(e) im S3",
      "type": "String",
      "value": "${app_file_context}/",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "filepaths",
        "target": "filepaths"
      },
      "constraints": {
        "notEmpty": true
      }
    }
  ]
}
